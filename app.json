{
  "expo": {
    "name": "DNS Chat",
    "slug": "chat-dns",
    "version": "4.0.6",
    "orientation": "default",
    "icon": "./icons/dnschat_ios26.png",
    "userInterfaceStyle": "automatic",
    "experiments": {
      "reactCompiler": true,
      "typedRoutes": true
    },
    "scheme": "dnschat",
    "ios": {
      "supportsTablet": true,
      "buildNumber": "34",
      "bundleIdentifier": "org.mvneves.dnschat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icons/dnschat_ios26.png",
        "backgroundColor": "#0D7377"
      },
      "versionCode": 34,
      "predictiveBackGestureEnabled": true,
      "package": "org.mvneves.dnschat"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge",
      "./plugins/dns-native-plugin",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0",
            "flipper": false,
            "ccacheEnabled": true,
            "generateStaticFrameworks": true,
            "generateSourcemap": true,
            "includeSymbolsInDsym": true,
            "xcodeproj": {
              "buildSettings": {
                "DEBUG_INFORMATION_FORMAT": "dwarf-with-dsym",
                "DSYM_FOLDER_PATH": "$(BUILT_PRODUCTS_DIR)/$(TARGET_NAME).app.dSYM",
                "DEPLOYMENT_POSTPROCESSING": "YES",
                "SEPARATE_STRIP": "YES",
                "STRIP_INSTALLED_PRODUCT": "YES",
                "COPY_PHASE_STRIP": "NO",
                "ENABLE_USER_SCRIPT_SANDBOXING": "NO"
              }
            }
          },
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "minSdkVersion": 24,
            "ndkVersion": "29.0.14206865"
          }
        }
      ],
      "expo-localization",
      "expo-router",
      "expo-secure-store"
    ]
  }
}
