{
  "name": "chat-dns",
  "version": "4.0.6",
  "description": "DNSChat - A React Native chat app using DNS TXT queries",
  "main": "entry.tsx",
  "author": "mvneves",
  "license": "MIT",
  "homepage": "https://github.com/mneves75/dnschat",
  "repository": {
    "type": "git",
    "url": "https://github.com/mneves75/dnschat.git"
  },
  "bugs": {
    "url": "https://github.com/mneves75/dnschat/issues"
  },
  "keywords": [
    "dns",
    "chat",
    "react-native",
    "expo",
    "llm",
    "dns-txt"
  ],
  "scripts": {
    "start": "expo start --dev-client",
    "web": "expo start --web",
    "android": "node scripts/run-android.js",
    "android:java24": "expo run:android",
    "ios": "expo run:ios",
    "sync-versions": "node scripts/sync-versions.js",
    "sync-versions:dry": "node scripts/sync-versions.js --dry-run",
    "fix-pods": "scripts/fix-cocoapods.sh",
    "clean-ios": "cd ios && rm -rf Pods/ build/ && pod install",
    "test": "jest --runInBand",
    "test:android": "jest --runInBand --testPathPattern=androidStartupDiagnostics",
    "verify:android": "node scripts/verify-android-setup.js",
    "verify:android-16kb": "node scripts/check-android-16kb.js",
    "verify:sdk-alignment": "node scripts/verify-sdk-alignment.js",
    "verify:typed-routes": "node scripts/verify-typed-routes.js",
    "verify:expo-doctor": "npx expo-doctor",
    "verify:react-compiler": "bunx react-compiler-healthcheck@1.0.0",
    "verify:all": "bun run verify:expo-doctor && bun run verify:sdk-alignment && bun run verify:typed-routes && bun run verify:dnsresolver-sync && bun run verify:ios-pods && bun run verify:react-compiler && bun run verify:android && bun run lint && bun run test",
    "verify:dnsresolver-sync": "node scripts/verify-dnsresolver-sync.js",
    "lint:ast-grep": "node scripts/run-ast-grep.js --config project-rules/astgrep-liquid-glass.yml",
    "lint": "bun run lint:ast-grep",
    "dns:harness:build": "tsc -p scripts/tsconfig.harness.json",
    "dns:harness": "bun run dns:harness:build && node scripts/dist/run-dns-harness.js",
    "verify:ios-pods": "node scripts/verify-ios-pods-sync.js",
    "prepare": "node scripts/install-git-hooks.js",
    "postinstall": "patch-package && node scripts/postinstall-ios-pods.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~55.0.6",
    "@noble/ciphers": "^2.1.1",
    "@noble/hashes": "^2.0.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-clipboard/clipboard": "^1.14.2",
    "@react-native-menu/menu": "^2.0.0",
    "@react-navigation/bottom-tabs": "^7.1.1",
    "@react-navigation/elements": "^2.2.3",
    "@react-navigation/native": "^7.0.12",
    "@react-navigation/native-stack": "^7.1.13",
    "@types/dns-packet": "^5.6.5",
    "date-fns": "^4.1.0",
    "dns-packet": "^5.6.1",
    "expo": "^55.0.0",
    "expo-asset": "~55.0.7",
    "expo-build-properties": "~55.0.9",
    "expo-constants": "~55.0.7",
    "expo-crypto": "~55.0.8",
    "expo-dev-client": "~55.0.9",
    "expo-glass-effect": "~55.0.7",
    "expo-haptics": "~55.0.8",
    "expo-linking": "~55.0.7",
    "expo-localization": "~55.0.8",
    "expo-router": "~55.0.2",
    "expo-secure-store": "~55.0.8",
    "expo-splash-screen": "~55.0.9",
    "expo-status-bar": "~55.0.4",
    "expo-system-ui": "~55.0.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-device-info": "^14.0.4",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-controller": "1.20.7",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "~5.7.0",
    "react-native-screens": "~4.23.0",
    "react-native-svg": "15.15.3",
    "react-native-tcp-socket": "^6.3.0",
    "react-native-udp": "^4.1.7",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@ast-grep/cli": "^0.40.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.2.10",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "markdown-it": ">=12.3.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-udp",
          "react-native-tcp-socket",
          "@dnschat/dns-native"
        ],
        "listUnknownPackages": true
      },
      "appConfigFieldsNotSyncedCheck": {
        "enabled": false
      }
    }
  },
  "private": true
}
